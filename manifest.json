{
  "manifest_version": 3,
  "name": "Wextension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "storage", "tabs", "activeTab"],
  "host_permissions": [
    "<all_urls>"
  ],
   "web_accessible_resources": [{
    "resources": ["icons/*.svg"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"],
      "type": "module"
    }
  ]
}
